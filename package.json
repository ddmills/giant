  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-stage-3": "^6.24.1",
    "parcel-bundler": "^1.6.2"
  },
  "dependencies": {
    "babel-preset-node8": "^1.2.0"
  }
}
