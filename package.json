{
  "name": "giant",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "server:dev": "npm run server:build && npm start",
    "client:dev": "parcel src/client/index.html -d dist/client",
    "client:build": "parcel build src/client/index.html -d dist/client",
    "server:build": "babel src/server -d dist/server -s",
    "build": "npm run client:build && npm run server:build"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "babel-preset-stage-3": "^6.24.1",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2"
  },
  "dependencies": {
    "config": "^1.29.4",
    "express": "^4.16.2",
    "socket.io": "^2.0.4"
  }
}
